    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.71.1"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
